<!-- Main Sidebar Container -->

<div class="content-wrapper" style="margin-left: 0px !important;">
  <router-outlet></router-outlet>
</div>



<button 
#modalHanhchinhPdfReference 
data-target="#modal-hanhchinh-pdf"
type="button" 
class="btn btn-default" 
data-toggle="modal" 
style="display: none;">
Mở Modal
</button>

<div class="modal fade" id="modal-hanhchinh-pdf">
  <div class="modal-dialog">
      <div class="modal-content">
          <div class="modal-header">
              <h4 class="modal-title">{{getNameLoaiDataIn()}}</h4>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
              </button>
          </div>
          <div class="modal-body">
              <div *ngIf="DataBase64Pdf" style="height: 79vh;">
                  <app-pdf-viewer [base64Src]="DataBase64Pdf"></app-pdf-viewer>
              </div>
          </div>
      </div>
      <!-- /.modal-content -->
  </div>
  <!-- /.modal-dialog -->
</div>
<!-- /.modal -->



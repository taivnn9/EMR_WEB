<!-- Content Wrapper. Contains page content -->


<!-- Main content -->
<section class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <!-- I Hành chính -->
        <div class="card card-primary card-outline card-custom">
          <!-- <div class="card-header">
              <h3 class="card-title"></h3>
            </div> -->
          <div class="row">
            <form class="form-horizontal col-6">
              <div class="card-body">
               
                <div class="form-group row">
                  <div class="col-8">
                    <div class="row">
                      <label class="col-3 col-form-label col-form-label-bold">III. KHÁM BỆNH</label>
                      <div class="col-4">
                        <input class="form-control form-control-sm" type="datetime-local" 
                          [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.NgayKhamBenh" [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                    </div>

                    <div class="row">
                      <label class="col-10 col-form-label col-form-label-bold">1. Toàn thân: </label>
                      <div class="col-2">
                        <div class="form-check">
                          <input type="checkbox" class="form-check-input" 
                          [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.PhuMe" 
                          [ngModelOptions]="{standalone: true}" id="DacDiemLienQuanBenhDiUng">
                          <!-- <input type="checkbox" class="form-check-input" id="DacDiemLienQuanBenhDiUng"> -->
                          <label class="form-check-label" for="DacDiemLienQuanBenhDiUng">Phù</label>
                        </div>
                      </div>
                    </div>

                    <div class="row">
                      <div class="col-12">
                        <textarea class="form-control" rows="9" placeholder=""
                          [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.ToanThan" 
                          [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                      </div>
                    </div>
                  </div>
                  <div class="col-4 form-background-custom">

                    <div class="row row-margin-custom">
                      <label class="col-4 col-form-label">Mạch</label>
                      <div class="col-5">
                        <input class="form-control form-control-sm" type="text" 
                        [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.DauSinhTon.Mach" 
                        [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                      <label class="col-3 col-form-label">lần/ph</label>
                      <!--  -->
                    </div>
                    <div class="row row-margin-custom">
                      <label class="col-4 col-form-label">Nhiệt độ</label>
                      <div class="col-5">
                        <input class="form-control form-control-sm" type="text" 
                        [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.DauSinhTon.NhietDo" 
                        [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                      <label class="col-3 col-form-label">ºC</label>
                      <!--  -->
                    </div>
                    <div class="row  row-margin-custom">
                      <!--  -->
                      <label class="col-4 col-form-label">Huyết áp</label>
                      <div class="col-5">
                        <input class="form-control form-control-sm" type="text" 
                        [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.DauSinhTon.HuyetAp" 
                        [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                      <label class="col-3 col-form-label">mmHg</label>
                      <!--  -->
                    </div>
                    <div class="row  row-margin-custom">
                      <!--  -->
                      <label class="col-4 col-form-label">Nhịp thở</label>
                      <div class="col-5">
                        <input class="form-control form-control-sm" type="text" 
                        [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.DauSinhTon.NhipTho" 
                        [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                      <label class="col-3 col-form-label">lần/ph</label>
                      <!--  -->
                    </div>
                    <div class="row  row-margin-custom">
                      <!--  -->
                      <label class="col-4 col-form-label">Cân nặng</label>
                      <div class="col-5">
                        <input class="form-control form-control-sm" type="text" 
                        [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.DauSinhTon.CanNang" 
                        [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                      <label class="col-3 col-form-label">kg</label>
                      <!--  -->
                    </div>
                    <div class="row  row-margin-custom">
                      <!--  -->
                      <label class="col-4 col-form-label">Chiều cao</label>
                      <div class="col-5">
                        <input class="form-control form-control-sm" type="text" 
                        [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.DauSinhTon.ChieuCao" 
                        [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                      <label class="col-3 col-form-label">cm</label>
                      <!--  -->
                    </div>
                    <div class="row  row-margin-custom">
                      <label class="col-4 col-form-label">BMI</label>
                      <div class="col-5">
                        <input class="form-control form-control-sm" type="text" 
                        [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.DauSinhTon.BMI" 
                        [ngModelOptions]="{standalone: true}" placeholder="">
                      </div>
                      <!--  -->
                    </div>


                  </div>

                </div>

                <!--  -->
                <div class="form-group row">
                  <label class="col-2 col-form-label ">+ Tuần hoàn:</label>
                  <div class="col-10">
                    <textarea class="form-control" rows="1" placeholder=""
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TuanHoan" 
                      [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label ">+ Hô hấp:</label>
                  <div class="col-10">
                    <textarea class="form-control" rows="1" placeholder=""
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.HoHap" 
                      [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label ">+ Tiêu hóa:</label>
                  <div class="col-10">
                    <textarea class="form-control" rows="1" placeholder=""
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TieuHoa" 
                      [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label ">+ Thận - Tiết niệu - Sinh dục:</label>
                  <div class="col-10">
                    <textarea class="form-control" rows="1" placeholder=""
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.ThanTietNieuSinhDuc" 
                      [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-2 col-form-label ">+ Khác:</label>
                  <div class="col-10">
                    <textarea class="form-control" rows="1" placeholder=""
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.Khac_CacCoQuan" 
                      [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>
                <!--  -->
                <div class="form-group row">
                  <label class="col-12 col-form-label col-form-label-bold">2. Khám ngoài: </label>
                </div>
                <div class="form-group row">
                  <label class="col-3 col-form-label ">+ Bụng có sẹo phẫu thuật cũ</label>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="checkbox" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.BungCoSeoPhauThuatCu" 
                      [ngModelOptions]="{standalone: true}" id="DacDiemLienQuanBenhDiUng">
                      <!-- <input type="checkbox" class="form-check-input" id="DacDiemLienQuanBenhDiUng"> -->
                      <label class="form-check-label" for="DacDiemLienQuanBenhDiUng"></label>
                    </div>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-3 col-form-label ">- Hình dạng TC: </label>
                  <div class="col-3">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.HinhDangTuCung" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-3 col-form-label ">- Tư thế: </label>
                  <div class="col-3">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TuThe" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-3 col-form-label ">- Chiều cao TC: </label>
                  <div class="col-3">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.ChieuCaoTC" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-3 col-form-label ">cm &nbsp; &nbsp; &nbsp; vòng bụng: </label>
                  <div class="col-3">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.VongBung" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-3 col-form-label ">- Cơn co TC: </label>
                  <div class="col-3">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.ConCoTC" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-3 col-form-label ">- Tim thai: </label>
                  <div class="col-3">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TimThai" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-3 col-form-label ">lần/phút </label>
                </div>

                <div class="form-group row">
                  <label class="col-3 col-form-label ">- Vú: </label>
                  <div class="col-9">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.Vu" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>
              </div>
            </form>

            <form class="form-horizontal col-6">
              <div class="card-body">
                <div class="form-group row">
                  <label class="col-12 col-form-label col-form-label-bold">3. Khám trong</label>
                </div>
                <div class="form-group row">
                  <label class="col-2 col-form-label">- Chỉ số Bishop</label>
                  <div class="col-2">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.ChiSoBishop" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">Điểm</label>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label">- Âm hộ: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.AmHo" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">- Âm đạo: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.AmDao" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label">- Cổ tử cung: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.CoTuCung" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">- Phần phụ: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.PhanPhu" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-2 col-form-label">- Tầng sinh môn: </label>
                  <div class="col-2">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TangSinhMon" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">- Tình trạng ối: </label>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TinhTrangOiID" [value]="1" [ngModelOptions]="{standalone: true}" id="TinhTrangOiID">
                      <label class="form-check-label" for="TinhTrangOiID">1.Ối phồng</label>
                    </div>
                  </div>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TinhTrangOiID"  [value]="2" [ngModelOptions]="{standalone: true}" id="TinhTrangOiID">
                      <label class="form-check-label" for="TinhTrangOiID">2.Ối dẹt</label>
                    </div>
                  </div>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TinhTrangOiID" [value]="3"  [ngModelOptions]="{standalone: true}" id="TinhTrangOiID">
                      <label class="form-check-label" for="TinhTrangOiID">3.Ối quả lê</label>
                    </div>
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-2 col-form-label">- Ối vỡ lúc: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="datetime-local" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.OiVoLuc" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">- Ối vỡ: </label>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.iOiVoID" [value]="1" [ngModelOptions]="{standalone: true}" id="DoPhauThuatID">
                      <label class="form-check-label" for="DoPhauThuatID">1.Tự nhiên</label>
                    </div>
                  </div>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.iOiVoID"  [value]="2" [ngModelOptions]="{standalone: true}" id="DoPhauThuatID">
                      <label class="form-check-label" for="DoPhauThuatID">2.Bấm ối</label>
                    </div>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label">- Màu sắc nước ối: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.MauSacNuocOi" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">- Nước ối nhiều hay ít: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.NuocOiNhieuHayIt" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                
                <div class="form-group row">
                  <label class="col-2 col-form-label">- Ngôi: </label>
                  <div class="col-2">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.Ngoi" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">- Thế: </label>
                  <div class="col-2">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.The_KhamTrong" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                  <label class="col-2 col-form-label">- Kiểu Thế: </label>
                  <div class="col-2">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.KieuThe" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                  
                <div class="form-group row">
                  <label class="col-2 col-form-label">- Độ lọt: </label>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.DoLotID" [value]="1" [ngModelOptions]="{standalone: true}" id="DoLotID">
                      <label class="form-check-label" for="DoLotID">1.Cao</label>
                    </div>
                  </div>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.DoLotID" [value]="2" [ngModelOptions]="{standalone: true}" id="DoLotID">
                      <label class="form-check-label" for="DoLotID">2.Chúc</label>
                    </div>
                  </div>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.DoLotID" [value]="3" [ngModelOptions]="{standalone: true}" id="DoLotID">
                      <label class="form-check-label" for="DoLotID">3.Chặt</label>
                    </div>
                  </div>
                  <div class="col-2">
                    <div class="form-check">
                      <input type="radio" class="form-check-input" 
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.DoLotID" [value]="4" [ngModelOptions]="{standalone: true}" id="DoLotID">
                      <label class="form-check-label" for="DoLotID">4. Lọt</label>
                    </div>
                  </div>

                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label">- Đường nhô hộ vệ: </label>
                  <div class="col-4">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.DuongKinhNhoHaVe" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>
                  
                <div class="form-group row">
                  <label class="col-12 col-form-label col-form-label-bold">4. Các xét nghiệm cận lâm sàng cần làm:</label>
                </div>

                <div class="form-group row">
                  <div class="col-12">
                    <textarea class="form-control" rows="3" placeholder=""
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.CacXetNghiemCanLamSangCanLam" 
                      [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-12 col-form-label col-form-label-bold">5. Chẩn đoán</label>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label ">- Khi vào khoa</label>
                  <div class="col-10">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.KhiVaoKhoa" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label ">- Phân biệt</label>
                  <div class="col-10">
                    <textarea class="form-control" rows="3" placeholder=""
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.PhanBiet" 
                    [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-12 col-form-label col-form-label-bold">6. Tiên lượng</label>
                </div>

                <div class="form-group row">
                  <div class="col-12">
                    <textarea class="form-control" rows="3" placeholder=""
                      [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.TienLuong" 
                      [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label col-form-label-bold">7. Hướng điều trị</label>
                  <div class="col-10">
                    <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.HuongDieuTri" [ngModelOptions]="{standalone: true}" placeholder="">
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-2 col-form-label ">Phương pháp chính</label>
                  <div class="col-10">
                    <textarea class="form-control" rows="3" placeholder=""
                    [(ngModel)]="ThongTinHoSoBenhAn.BenhAn.PhuongPhapChinh" 
                    [ngModelOptions]="{standalone: true}" placeholder=""></textarea>
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-6 col-form-label col-form-label-bold col-form-label-algin"></label>
                  <label class="col-6 col-form-label col-form-label-bold col-form-label-algin">Bác sĩ làm bệnh án</label>
                </div>
                <div class="form-group row">
                  
                  <div class="col-6"></div>
                  <div class="col-6">
                      
                    <virtual-scroll-example   
                      [items]="ListNhanVien"
                      [value]="ThongTinHoSoBenhAn.ThongTinDieuTri.MaTruongKhoa"
                      (valueChange)="ThongTinHoSoBenhAn.ThongTinDieuTri.MaTruongKhoa=$event"
                    ></virtual-scroll-example>
                    <!-- <input class="form-control form-control-sm" type="text" 
                    [(ngModel)]="ThongTinHoSoBenhAn.ThongTinDieuTri.MaTruongKhoa" [ngModelOptions]="{standalone: true}" placeholder=""> -->
                  </div>
                </div>
                <!--  -->
              </div>
              <!-- /.card-body -->
            </form>

          </div>
          <!-- /.card-body -->
        </div>
      </div>

    </div>
  </div><!-- /.container-fluid -->
</section>
<!-- /.content -->